//$name
#define EXT$name(msg) throw $namspaces$name(msg,this->getNameAndType(),__FILE__,__func__,__LINE__)
#define EXT$nameO(msg, owner) throw $namspaces$name(msg,owner,__FILE__,__func__,__LINE__)
#define EXT$nameI(msg, inner) throw $namspaces$name(msg,this->getNameAndType(),__FILE__,__func__,__LINE__,inner)
#define EXT$nameIO(msg, owner, inner) throw $namspaces$name(msg,owner,__FILE__,__func__,__LINE__,inner)
